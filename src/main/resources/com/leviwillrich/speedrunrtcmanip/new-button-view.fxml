<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import jfxtras.scene.control.LocalDateTimePicker?>
<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.leviwillrich.speedrunrtcmanip.NewButtonController">
   <HBox prefHeight="100.0" prefWidth="200.0">
      <Label text="Name:"/>
      <TextField fx:id="name"/>
   </HBox>
   <HBox prefHeight="100.0" prefWidth="200.0">
      <Label text="System Time:"/>
      <LocalDateTimePicker fx:id="dateTimePicker"/>
   </HBox>
   <Button mnemonicParsing="false" text="Add" onAction="#onAddButtonClick"/>
</VBox>
